    "codecov.io": "^0.1.6",
    "cross-env": "^5.0.0",
    "del": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-ngryman": "^1.7.1",
    "get-stream": "^3.0.0",
    "mock-require": "^2.0.2",
    "nyc": "^11.2.1",
    "sinon": "^4.0.1"
