    "cli-truncate": "^1.0.0",
    "debug": "^3.1.0",
    "delay": "^2.0.0",
    "devtools-timeline-model": "^1.3.0",
    "globby": "^6.1.0",
    "indent-string": "^3.1.0",
    "locate-chrome": "^0.1.1",
    "meow": "^3.7.0",
    "mkdirp": "^0.5.1",
    "ms": "^2.0.0",
    "p-map-series": "^1.0.0",
    "p-series": "^1.0.0",
    "p-waterfall": "^1.0.0",
    "pify": "^3.0.0",
